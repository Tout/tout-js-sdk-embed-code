{
  extends: "eslint:recommended",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "TOUT": true
  },
  "rules": {
    "quotes": [2, "single"],
    "no-use-before-define": [2, "nofunc"],
    "no-mixed-spaces-and-tabs": 2,
    "indent": [2, 2],
    "no-unused-vars": 1,
    "no-console": 2,
    "no-underscore-dangle": 0,
    "no-alert": 2,
    "semi": 2,
    "no-unexpected-multiline": 2
  }
}
